# Frontend Configuration
VITE_APP_NAME="Axioma Mini"

# Development - API Base URL (relative path works with Vite proxy)
VITE_API_URL=/api

# Production - Update this in .env.production with your production API URL
# IMPORTANT: Do NOT include port numbers in production URLs (use proxy/nginx instead)
# Example for production: VITE_API_URL=https://yourdomain.com/api
# Or if same domain: VITE_API_URL=/api

VITE_PORT=8088
